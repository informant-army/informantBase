<plugin name='domain-plugin' version='0.1' grailsVersion='2.2.3 &gt; *'>
  <author>Your name</author>
  <title>Domain Plugin Plugin</title>
  <description>Brief summary/description of the plugin.
</description>
  <documentation>http://grails.org/plugin/domain-plugin</documentation>
  <type>DomainPluginGrailsPlugin</type>
  <resources>
    <resource>br.com.informant.domain.Role</resource>
    <resource>br.com.informant.domain.User</resource>
    <resource>br.com.informant.domain.UserRole</resource>
    <resource>br.com.informant.domain.Usuario</resource>
    <resource>br.com.informant.domain.Laticinio</resource>
    <resource>br.com.informant.domain.Produtor</resource>
    <resource>br.com.informant.domain.Transportador</resource>
    <resource>br.com.informant.domain.RoleService</resource>
    <resource>br.com.informant.domain.UserRoleService</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='https://nexus.codehaus.org/content/repositories/snapshots' url='https://nexus.codehaus.org/content/repositories/snapshots/' />
  </repositories>
  <dependencies>
    <runtime>
      <dependency group='mysql' name='mysql-connector-java' version='5.1.22' />
      <dependency group='postgresql' name='postgresql' version='9.1-901.jdbc4' />
    </runtime>
  </dependencies>
  <plugins>
    <compile>
      <plugin group='org.grails.plugins' name='rest' version='0.7' />
      <plugin group='org.grails.plugins' name='spring-security-core' version='1.2.7.3' />
      <plugin group='org.grails.plugins' name='mail' version='1.0.1' />
      <plugin group='org.grails.plugins' name='cache' version='1.0.1' />
      <plugin group='org.grails.plugins' name='spring-security-cas' version='1.0.5' />
    </compile>
    <runtime>
      <plugin group='org.grails.plugins' name='hibernate' version='2.2.3' />
      <plugin group='org.grails.plugins' name='resources' version='1.2' />
      <plugin group='org.grails.plugins' name='jquery' version='1.8.3' />
    </runtime>
    <build>
      <plugin group='org.grails.plugins' name='tomcat' version='2.2.3' />
    </build>
  </plugins>
  <runtimePluginRequirements />
  <behavior />
</plugin>